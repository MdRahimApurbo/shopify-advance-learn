{% if section.settings.show_annoucement %}
  <section>Annoucement Bar</section>
{% endif %}

{% schema %}
{
  "name": "Annoucement Bar",
  "settings": [
    {
      "type": "checkbox",
      "id": "show_annoucement",
      "label": "Show Annoucement",
      "default": true
    }
  ],
  "presets": []
}
{% endschema %}
